<div class="slide-show-container" id="slideshow">
  <div class="wrap">
    <div class="slide active">
      <img src="dist/img/banner1.jpg" alt="banner">
    </div>

    <div class="slide">
      <img src="dist/img/banner2.jpg" alt="banner">
    </div>

    <div class="slide">
      <img src="dist/img/banner3.jpg" alt="banner">
    </div>
    <ol class="slide-indicators">
      <li data-target="#slideshow" data-slide-to="0"></li>
      <li data-target="#slideshow" data-slide-to="1"></li>
      <li data-target="#slideshow" data-slide-to="2"></li>
    </ol>
  </div>
</div>
